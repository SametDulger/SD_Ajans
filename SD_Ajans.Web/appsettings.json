{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=SD_Ajans.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FileUpload": {
    "MaxFileSizeInMB": 5,
    "AllowedExtensions": [".jpg", ".jpeg", ".png", ".gif", ".webp"],
    "UploadPath": "uploads"
  },
  "Security": {
    "SessionTimeoutInMinutes": 480,
    "MaxLoginAttempts": 5,
    "LockoutDurationInMinutes": 15
  },
  "Application": {
    "Name": "SD Ajans",
    "Version": "1.0.0",
    "Environment": "Development"
  }
}
